<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    tools:context=".network.Ui.PhotoFragment">

    <TextView
        android:id="@+id/photo_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="사진"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="@color/black"
        android:textSize="35sp" />

    <View
        android:id="@+id/view_1"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:background = "@drawable/voice_state_btn"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/photo_title" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="20dp"
        android:src="@drawable/image_1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/photo_title"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-11-25(수요일)"
        android:layout_marginRight="30dp"
        android:textSize="20sp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@id/photo_title"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor ="@color/black" />

    <View
        android:id="@+id/view_2"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:background = "@drawable/voice_state_btn"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/view_1" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="20dp"
        android:src="@drawable/image_2_n"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_1"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-11-25(수요일)"
        android:layout_marginRight="30dp"
        android:textSize="20sp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@id/view_1"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor ="@color/black" />

    <View
        android:id="@+id/view_3"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:background = "@drawable/voice_state_btn"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/view_2" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="20dp"
        android:src="@drawable/image_3"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_2"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-11-24(화요일)"
        android:layout_marginRight="30dp"
        android:textSize="20sp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@id/view_2"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor ="@color/black" />

    <View
        android:id="@+id/view_4"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:background = "@drawable/voice_state_btn"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/view_3" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="20dp"
        android:src="@drawable/image_4"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_3"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-11-24(화요일)"
        android:layout_marginRight="30dp"
        android:textSize="20sp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@id/view_3"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor ="@color/black" />

    <View
        android:id="@+id/view_5"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:background = "@drawable/voice_state_btn"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/view_4" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="20dp"
        android:src="@drawable/image_5"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_4"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-11-24(화요일)"
        android:layout_marginRight="30dp"
        android:textSize="20sp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@id/view_4"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor ="@color/black" />
    <View
        android:id="@+id/view_6"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:background = "@drawable/voice_state_btn"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/view_5" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="20dp"
        android:src="@drawable/image_5"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_5"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-11-24(화요일)"
        android:layout_marginRight="30dp"
        android:textSize="20sp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@id/view_5"
        app:layout_constraintRight_toRightOf="parent"
        android:textColor ="@color/black" />

</androidx.constraintlayout.widget.ConstraintLayout>